<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gaoshu.mapper.QiCheTuPianMapper">
    <select id="selectUrlsByPpxhidAndTplx" parameterType="Map" resultType="String">
        select <if test="tplx==1">distinct</if> url from qichetupian where ppxhid=#{ppxhid} and tplx=#{tplx}
    </select>
</mapper>